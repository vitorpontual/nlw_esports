{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": "./src",
    "baseUrl": "./src",
    "outDir": "./dist",
    /*"paths": {
      "@modules/*": ["modules/*"],
      "@config/*": [ "config/*" ],
      "@shared/*": ["shared/*"],
      "@errors/*": ["errors/*"],
      "@utils": ["utils/*"]
    },*/
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}